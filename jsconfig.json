{
	"compilerOptions": {
		"experimentalDecorators": true,
		"checkJs": false,
		"target": "esnext",
		"baseUrl": ".",
		"moduleResolution": "node",
		"jsx": "react",
		"paths": {
			"@store": [
				"src/store/index"
			],
			"@screens": [
				"src/screens/index"
			],
			"@components": [
				"src/components/index"
			],
