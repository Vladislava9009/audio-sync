{
  "name": "audioasync",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "clean": "rimraf $TMPDIR/react-* && yarn cache clean --force && yarn android:clean && yarn ios:clean && rimraf $TMPDIR/react-native-packager-cache-* && rimraf $TMPDIR/metro-bundler-cache-* && rimraf $TMPDIR/metro* && watchman watch-del-all",
    "ios": "react-native run-ios",
    "android": "react-native run-android",
    "android:reverse": "adb reverse tcp:8081 tcp:8081",
    "android:menu": "adb shell input keyevent KEYCODE_MENU",
    "android:reload": "adb shell input text \"RR\"",
    "g:component": "yarn hygen generate component",
    "g:screen": "yarn hygen generate screen",
    "g:helper": "yarn hygen generate helper",
    "g:hook": "yarn hygen generate hook",
    "g:reducer": "yarn hygen generate reducer",
    "i18n:scan": "i18next-scanner 'src/**/*.{js,jsx,ts,tsx}'",
    "packager:kill": "react-native-kill-packager",
    "pod:install": "yarn && cd ios && pod install && cd ..",
    "android:build": "yarn jetifier && cd android && ./gradlew assembleProductionRelease && cd .. && open ./android/app/build/outputs/apk/production/release/",
    "android:build-beta": "yarn jetifier && cd android && ./gradlew assembleStagingRelease && cd .. && open ./android/app/build/outputs/apk/staging/release/",
    "android:install-release": "yarn jetifier && cd android && ./gradlew installProductionDebug && cd ..",
    "android:install-beta": "yarn jetifier && cd android && ./gradlew installStagingDebug && cd ..",
    "android:devices": "adb devices",
    "android:logcat": "react-native log-android",
    "android:clean": "cd android/ && ./gradlew clean && cd ..",
    "android:keytool": "keytool -genkeypair -v -keystore ./android/app/release.keystore -alias key-alias -keyalg RSA -keysize 2048 -validity 10000",
    "ios:6plus": "react-native run-ios --simulator=\"iPhone 6 Plus\"",
    "ios:xsmax": "react-native run-ios --simulator=\"iPhone XS Max\"",
    "ios:log": "react-native log-ios",
    "ios:devices": "xcrun simctl list devices",
    "ios:clean": "rimraf ./ios/build",
    "ios:bundle": "yarn react-native bundle --entry-file index.js --platform ios --dev false --bundle-output ios/main.jsbundle",
    "lint": "tslint src/**/*.{js,jsx,ts,tsx} --fix",
    "prettier": "prettier --config ./.prettierrc --write  \"./src/**/*.{js,jsx,ts,tsx}\"",
    "git-log": "git log --pretty=format:\"%h - %an, %ar : %s\"",
    "version": "yarn changelog && git add CHANGELOG.md",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "analyze:jscpd": "jscpd ./src",
    "analyze:bundle": "react-native-bundle-visualizer --bundle-output ./reports/app.bundle",
    "postversion": "react-native-version && git push --tags"
  },
  "dependencies": {
    "react": "17.0.1",
    "react-native": "0.64.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-optional-chaining": "^7.2.0",
    "@babel/runtime": "^7.12.5",
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/axios": "^0.14.0",
    "@types/i18next": "^12.1.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.121",
    "@types/luxon": "^1.12.0",
    "@types/ptomasroos__react-native-multi-slider": "^0.0.1",
    "@types/react": "^16.8.5",
    "@types/react-i18next": "^8.1.0",
    "@types/react-native": "^0.64.5",
    "@types/react-native-fbsdk": "^1.0.0",
    "@types/react-native-keyboard-spacer": "^0.4.1",
    "@types/react-native-multi-slider": "^0.3.1",
    "@types/react-native-platform-touchable": "^1.1.1",
    "@types/react-native-read-more-text": "^1.0.0",
    "@types/react-native-scrollable-tab-view": "^0.10.1",
    "@types/react-native-share": "^1.1.4",
    "@types/react-native-snap-carousel": "^3.7.4",
    "@types/react-native-sqlite-storage": "^3.3.2",
    "@types/react-navigation": "^3.0.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/validator": "^10.11.3",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "conventional-changelog-cli": "^2.0.23",
    "enquirer": "^2.3.1",
    "eslint": "^7.14.0",
    "husky": "^3.0.2",
    "hygen": "^4.0.7",
    "i18next-scanner": "^2.10.2",
    "jest": "^26.6.3",
    "jetifier": "^1.6.6",
    "jscpd": "^2.0.16",
    "lint-staged": "^9.2.1",
    "metro-react-native-babel-preset": "^0.64.0",
    "prettier": "^1.19.1",
    "react-native-bundle-visualizer": "^2.0.4",
    "react-native-css-transformer": "^1.2.4",
    "react-native-kill-packager": "^1.0.0",
    "react-native-rename": "^2.4.1",
    "react-native-safe-area-context": "^3.1.9",
    "react-native-svg-transformer": "^0.13.0",
    "react-native-typescript-transformer": "^1.2.13",
    "react-native-version": "^3.1.0",
    "react-test-renderer": "17.0.1",
    "redux": "^4.0.4",
    "rimraf": "^2.6.3",
    "solidarity": "^2.3.1",
    "solidarity-react-native": "^2.1.2",
    "source-map-explorer": "^2.1.1",
    "stylelint": "^10.1.0",
    "stylelint-config-react-native-css-modules": "^2.5.0",
    "stylelint-react-native": "^2.1.1",
    "tslint": "^5.20.1",
    "tslint-config-airbnb": "^5.11.2",
    "tslint-config-prettier": "^1.18.0",
    "tslint-plugin-prettier": "^2.0.1",
    "tslint-react": "^4.1.0",
    "typescript": "^3.8.3"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}